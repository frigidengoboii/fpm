{
    "is_fpm_config_file": true,
    "example_setup_formatted_for": "USYD Love Letters Revived",
    "fpm": {
        "state_file": "fpm.state.pickle",
	"post_format": "templates/love_letters_revived.tmpl",
        "timestamp_format": "%d/%m/%Y %H:%M"
    },
    "fb_api": {
        "app_id": "<facebook app id>",
        "app_secret": "<facebook app secret>",
        "client_token": "<facebook client token>",
        "page_id": "<facebook page id>"
    },
    "sheets_api": {
        "api_key": "<google api key>",
        "spreadsheet_id": "<google sheets id>",
        "column_range": "A:B",
        "column_mappings": 
            ["timestamp", "content"]
    }
}
